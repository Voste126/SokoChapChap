extends Layout
include ../mixins/navbars/NavbarWatch
include ../mixins/Variables

prepend scripts
  script(src=`https://polyfill.io/v3/polyfill.min.js?features=window.scroll`)
  script(src=`${CWD}vendors/feather-icons/feather.min.js`)
  script.
    feather.replace();
 
 
append footerStyles
  link(href='https://fonts.googleapis.com/css2?family=Roboto+Condensed:wght@300;700&display=swap' rel='stylesheet')

block mainContent
  +NavbarWatch
  block wrapperContent 
    block beforeContent
    block content
    block afterContent

